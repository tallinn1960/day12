                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=day12"
                ],
                "filter": {
                    "name": "day12",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
